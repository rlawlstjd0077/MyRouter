<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import com.jfoenix.controls.JFXButton?>
<fx:root prefHeight="400.0" prefWidth="600.0" stylesheets="@../main.css" type="BorderPane" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1">
    <center>
        <VBox>
            <padding>
                <Insets bottom="100" left="20" right="20" top="20" />
            </padding>
            <TableView>
                <columns>
                    <TableColumn prefWidth="20" />
                    <TableColumn prefWidth="100" text="StartTime" />
                    <TableColumn prefWidth="100" text="EndTime" />
                </columns>
                <columnResizePolicy>
                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                </columnResizePolicy>
            </TableView>
            <HBox>
            </HBox>
        </VBox>
    </center>
    <bottom>
        <HBox alignment="CENTER">
            <padding>
                <Insets bottom="20"/>
            </padding>
            <JFXButton text="Save" styleClass="handle-button" prefWidth="80" fx:id="saveBtn"/>
            <Label prefWidth="20"/>
            <JFXButton text="Cancel" styleClass="handle-button" prefWidth="80" fx:id="cancelBtn"/>
        </HBox>
    </bottom>
</fx:root>